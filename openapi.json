          "schema" : {
            "type" : "string",
            "enum" : [ "ard", "br", "swr", "ndr", "hr", "wdr", "mdr", "rbb" ]
          }
        }, {
          "name" : "roadName",
          "in" : "path",
          "description" : "Comma, separated and url encoded list of road names, like A5 or B10. Capitalization does not matter, however uppercase is preferred.",
